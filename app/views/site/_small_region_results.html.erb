<ul class="nav nav-tabs" role="tablist">
  <li role="presentation" class="active" id="sm-region-circos-nav"><%= link_to "Circos Plot".html_safe, '#sm-circos', role: 'tab', 'data-toggle' => 'tab' %></li>
  <li role="presentation" id="sm-region-table-nav"><%= link_to "Raw Results".html_safe, '#sm-table', role: 'tab', 'data-toggle' => 'tab' %></li>
</ul>
<div id="sm-circos-results-tabs" class="tab-content">
  <div id="sm-circos" class="tab-pane pad active fade in">
    <div class="row">
      <div class="col-md-12">
        <div id="sm-biocircos-target" class="text-center"></div>
      </div>
    </div>
  </div>
  <div id="sm-table" class="tab-pane pad fade">
    <div class="row">
      <div class="col-md-12">
        <table id="sm-region-table" class="table table-striped table-condensed">
          <thead>
          <tr>
            <th>CNV ID</th>
            <th>Chrom</th>
            <th>Start</th>
            <th>End</th>
            <th>GSCN Min</th>
            <th>GSN Non Ref</th>
            <th>GSCN Category</th>
          </tr>
          </thead>
          <tbody>
          <% @table_data.each do |region| %>
            <tr>
              <td><%= region.cnv_id %></td>
              <td><%= region.chrom %></td>
              <td><%= region.start %></td>
              <td><%= region.end %></td>
              <td><%= region.gscn_min %></td>
              <td><%= region.gsn_non_ref %></td>
              <td><%= region.gscn_category %></td>
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
